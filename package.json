{
  "name": "profits-gg-turborepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build:customers-wise": "turbo run build --filter=customers-wise",
    "dev:customers-wise": "turbo dev --scope=\"@profits-gg/customers-wise\"",
    "db:customers-wise:generate": "turbo db:generate --scope=\"@profits-gg/customers-wise\"",
    "db:customers-wise:push": "turbo db:generate db:push --scope=\"@profits-gg/customers-wise\"",
    "clean": "rm -rf node_modules",
    "clean:workspaces": "turbo clean",
    "dev:data-x": "turbo dev --scope=\"@profits-gg/data-x\"",
    "build:data-x": "turbo run build --filter=data-x",
    "db:data-x:generate": "turbo db:generate --scope=\"@profits-gg/data-x\"",
    "db:data-x:push": "turbo db:generate db:push --scope=\"@profits-gg/data-x\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "FIXME:lint": "turbo lint && manypkg check",
    "type-check": "turbo type-check"
  },
  "dependencies": {
    "@manypkg/cli": "^0.19.2",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  }
}
